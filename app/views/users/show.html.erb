<h2>Created Events by <%= link_to @user.email, user_path(@user) %></h2>
<ul>
  <% @user.created_events.each do |event| %>
    <li><%= link_to event.title, event_path(event)%><br>date: <%= event.event_date%><br><%=event.event_location%>
    </li>
  <% end %>
</ul>
<%if user_signed_in?%>
  <p><%= link_to "Create New Event", new_event_path%></p>
<%end%>

<h2>Attending Events by <%= link_to @user.email, user_path(@user) %></h2>
<ul>
  <% @user.attended_events.each do |event| %>
    <li><%= link_to event.title, event_path(event)%><br>date: <%= event.event_date%><br><%=event.event_location%>
    </li>
  <% end %>
</ul>
